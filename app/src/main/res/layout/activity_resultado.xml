            android:layout_marginBottom="260dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.811"
    />
    <TextView
            android:text="Sexo:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="534dp"
            tools:layout_editor_absoluteX="245dp" android:id="@+id/textView8"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="575dp"
            tools:layout_editor_absoluteX="245dp" android:id="@+id/textView9"/>
